{
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)\\.js", "destination": "/$1.js" },
    { "source": "/(.*)\\.css", "destination": "/$1.css" },
    { "source": "/(.*)\\.(png|jpg|jpeg|webp|gif|svg|ico|mp3)", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}

